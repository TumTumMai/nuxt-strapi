{
  "kind": "collectionType",
  "collectionName": "movies",
  "info": {
    "name": "Movie"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "category": {
      "model": "category",
      "via": "movies"
    },
    "content": {
      "type": "richtext"
    },
    "description": {
      "type": "text"
    }
  }
}
